    "require": {
        "php": ">= 7.4",
        "symfony/http-foundation": "^5.0",
        "symfony/config": "^5.0",
        "php-di/php-di": "^6.0",
        "symfony/routing": "^5.0",
        "monolog/monolog": "^2.2",
        "psr/log": "^1.1",
